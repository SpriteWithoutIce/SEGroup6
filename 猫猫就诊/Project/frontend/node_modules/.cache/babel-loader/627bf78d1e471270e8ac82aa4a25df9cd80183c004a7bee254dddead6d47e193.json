{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      isVisible: false,\n      formData: {\n        patientName: '',\n        consultationDate: '',\n        patientGender: 'male',\n        patientAge: '',\n        medicines: [{\n          name: '',\n          quantity: ''\n        }]\n      }\n    };\n  },\n  methods: {\n    openModal() {\n      this.isVisible = true;\n    },\n    closeModal() {\n      this.isVisible = false;\n    },\n    addMedicine() {\n      this.formData.medicines.push({\n        name: '',\n        quantity: ''\n      });\n    },\n    removeMedicine(index) {\n      this.formData.medicines.splice(index, 1);\n    },\n    submitForm() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          console.log('Form is valid');\n          console.log(this.formData);\n          this.isVisible = false;\n        } else {\n          console.log('Form is invalid');\n          return false;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","isVisible","formData","patientName","consultationDate","patientGender","patientAge","medicines","name","quantity","methods","openModal","closeModal","addMedicine","push","removeMedicine","index","splice","submitForm","$refs","form","validate","valid","console","log"],"sources":["E:\\2024年春\\SoftwareEngineering\\se_group6_project\\zqs_project_1\\src\\components\\PrescriptionDetails.vue"],"sourcesContent":["<template>\r\n  <el-dialog :visible.sync=\"isVisible\" title=\"Prescription Details\">\r\n    <el-form ref=\"form\" :model=\"formData\" label-width=\"100px\">\r\n      <el-form-item label=\"Patient Name\" prop=\"patientName\">\r\n        <el-input v-model=\"formData.patientName\" placeholder=\"Enter patient name\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Consultation Date\" prop=\"consultationDate\">\r\n        <el-date-picker v-model=\"formData.consultationDate\" type=\"date\"\r\n          placeholder=\"Select consultation date\"></el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"Patient Gender\" prop=\"patientGender\">\r\n        <el-radio-group v-model=\"formData.patientGender\">\r\n          <el-radio label=\"male\">Male</el-radio>\r\n          <el-radio label=\"female\">Female</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      <el-form-item label=\"Patient Age\" prop=\"patientAge\">\r\n        <el-input-number v-model=\"formData.patientAge\" :min=\"0\" placeholder=\"Enter patient age\"></el-input-number>\r\n      </el-form-item>\r\n      <el-form-item label=\"Medication/Quantity\" prop=\"medicines\">\r\n        <div v-for=\"(medicine, index) in formData.medicines\" :key=\"index\" class=\"medicine-row\">\r\n          <el-input v-model=\"medicine.name\" placeholder=\"Enter medicine\" style=\"width: 200px;\"></el-input>\r\n          <el-input v-model=\"medicine.quantity\" placeholder=\"Enter quantity\" style=\"width: 200px;\"></el-input>\r\n          <el-button type=\"text\" @click=\"removeMedicine(index)\"\r\n            v-if=\"index !== formData.medicines.length - 1\">Remove</el-button>\r\n        </div>\r\n        <el-button @click=\"addMedicine\" plain>Add Medicine</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"closeModal\">Cancel</el-button>\r\n      <el-button type=\"primary\" @click=\"submitForm\">Submit</el-button>\r\n    </span>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      isVisible: false,\r\n      formData: {\r\n        patientName: '',\r\n        consultationDate: '',\r\n        patientGender: 'male',\r\n        patientAge: '',\r\n        medicines: [{ name: '', quantity: '' }]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    openModal () {\r\n      this.isVisible = true;\r\n    },\r\n    closeModal () {\r\n      this.isVisible = false;\r\n    },\r\n    addMedicine () {\r\n      this.formData.medicines.push({ name: '', quantity: '' });\r\n    },\r\n    removeMedicine (index) {\r\n      this.formData.medicines.splice(index, 1);\r\n    },\r\n    submitForm () {\r\n      this.$refs.form.validate(valid => {\r\n        if (valid) {\r\n          console.log('Form is valid');\r\n          console.log(this.formData);\r\n          this.isVisible = false;\r\n        } else {\r\n          console.log('Form is invalid');\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.medicine-row {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>\r\n"],"mappings":";AAqCA,eAAe;EACbA,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,gBAAgB,EAAE,EAAE;QACpBC,aAAa,EAAE,MAAM;QACrBC,UAAU,EAAE,EAAE;QACdC,SAAS,EAAE,CAAC;UAAEC,IAAI,EAAE,EAAE;UAAEC,QAAQ,EAAE;QAAG,CAAC;MACxC;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAAQA,CAAA,EAAK;MACX,IAAI,CAACV,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDW,UAASA,CAAA,EAAK;MACZ,IAAI,CAACX,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDY,WAAUA,CAAA,EAAK;MACb,IAAI,CAACX,QAAQ,CAACK,SAAS,CAACO,IAAI,CAAC;QAAEN,IAAI,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC,CAAC;IAC1D,CAAC;IACDM,cAAaA,CAAGC,KAAK,EAAE;MACrB,IAAI,CAACd,QAAQ,CAACK,SAAS,CAACU,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAC1C,CAAC;IACDE,UAASA,CAAA,EAAK;MACZ,IAAI,CAACC,KAAK,CAACC,IAAI,CAACC,QAAQ,CAACC,KAAI,IAAK;QAChC,IAAIA,KAAK,EAAE;UACTC,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;UAC5BD,OAAO,CAACC,GAAG,CAAC,IAAI,CAACtB,QAAQ,CAAC;UAC1B,IAAI,CAACD,SAAQ,GAAI,KAAK;QACxB,OAAO;UACLsB,OAAO,CAACC,GAAG,CAAC,iBAAiB,CAAC;UAC9B,OAAO,KAAK;QACd;MACF,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}