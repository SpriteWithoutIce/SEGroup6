{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nconst medicineDatabase = [{\n  name: 'Paracetamol',\n  quantity: '10 tablets'\n}, {\n  name: 'Ibuprofen',\n  quantity: '20 tablets'\n}, {\n  name: 'Aspirin',\n  quantity: '30 tablets'\n}, {\n  name: 'Amoxicillin',\n  quantity: '15 capsules'\n}, {\n  name: 'Omeprazole',\n  quantity: '10 capsules'\n}\n// 添加更多药物数据...\n];\nexport default {\n  name: 'PrescriptionDetails',\n  data() {\n    return {\n      isVisible: false,\n      patientName: '',\n      consultationDate: '',\n      patientGender: 'male',\n      patientAge: '',\n      medicines: [{\n        name: '',\n        quantity: ''\n      }],\n      medicineDatabase: medicineDatabase\n    };\n  },\n  methods: {\n    openModal() {\n      this.isVisible = true;\n    },\n    closeModal() {\n      this.isVisible = false;\n    },\n    addMedicine() {\n      this.medicines.push({\n        name: '',\n        quantity: ''\n      });\n    },\n    searchMedicine(index) {\n      const searchValue = this.medicines[index].name.toLowerCase();\n      const foundMedicine = this.medicineDatabase.find(medicine => medicine.name.toLowerCase() === searchValue);\n      if (foundMedicine) {\n        this.medicines[index].quantity = foundMedicine.quantity;\n      } else {\n        this.medicines[index].quantity = '';\n      }\n    },\n    submitForm() {\n      // Here you can handle form submission, such as sending data to server\n      // For now, let's just log the form data\n      console.log({\n        patientName: this.patientName,\n        consultationDate: this.consultationDate,\n        patientGender: this.patientGender,\n        patientAge: this.patientAge,\n        medicines: this.medicines\n      });\n      this.isVisible = false; // Close modal after form submission\n    }\n  }\n};","map":{"version":3,"names":["medicineDatabase","name","quantity","data","isVisible","patientName","consultationDate","patientGender","patientAge","medicines","methods","openModal","cl