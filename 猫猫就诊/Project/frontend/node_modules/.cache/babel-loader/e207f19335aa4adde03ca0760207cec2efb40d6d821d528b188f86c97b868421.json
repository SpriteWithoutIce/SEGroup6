{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'PrescriptionDetails',\n  data() {\n    return {\n      isVisible: false,\n      form: {\n        name: '',\n        date: '',\n        delivery: false,\n        gender: [],\n        advice: '',\n        medicines: [{\n          name: '',\n          quantity: 1,\n          price: 0,\n          totalPrice: 0\n        }]\n      },\n      medicinesDB: [{\n        name: '阿司匹林',\n        stock: 100,\n        price: 2.5\n      }, {\n        name: '头孢',\n        stock: 50,\n        price: 10.0\n      }, {\n        name: '布洛芬',\n        stock: 80,\n        price: 5.0\n      }\n      // 可以添加更多药物数据\n      ]\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.form.medicines.reduce((total, medicine) => total + parseFloat(medicine.totalPrice || 0), 0).toFixed(2);\n    }\n  },\n  methods: {\n    openModal() {\n      this.isVisible = true;\n      document.body.style.overflow = 'hidden'; // 禁止滚动\n    },\n    closeModal() {\n      this.isVisible = false;\n      document.body.style.overflow = ''; // 恢复滚动\n    },\n    submitForm() {\n      // 提交表单逻辑\n      console.log('提交表单');\n      // 提交后关闭弹窗\n      this.closeModal();\n    },\n    addMedicine() {\n      this.form.medicines.push({\n        name: '',\n        quantity: 1,\n        price: 0,\n        totalPrice: 0\n      });\n    },\n    removeMedicine(index) {\n      this.form.medicines.splice(index, 1);\n    },\n    calculatePrice(medicine) {\n      const selectedMedicine = this.medicinesDB.find(med => med.name === medicine.name);\n      if (selectedMedicine) {\n        medicine.price = selectedMedicine.price;\n        medicine.totalPrice = (selectedMedicine.price * medicine.quantity).toFixed(2);\n      }\n    },\n    querySearch(queryString, cb) {\n      var medicines = this.medicinesDB.map(item => ({\n        value: item.name\n      })); // 现在每个条目都是一个对象，包含 value 属性\n      console.log(medicines);\n      var results = queryString ? medicines.filter(this.createFilter(queryString)) : medicines;\n      console.log(results);\n      // 调用回调函数返回建议的结果\n      cb(results);\n    },\n    createFilter(queryString) {\n      return medicine => {\n        return medicine.value.toLowerCase().includes(queryString.toLowerCase());\n      };\n    },\n    handleSelect(item) {\n      var selectedMedicine = this.medicinesDB.find(med => med.name === item);\n      if (selectedMedicine) {\n        this.form.medicines[this.form.medicines.length - 1].price = selectedMedicine.price;\n        this.calculatePrice(this.form.medicines[this.form.medicines.length - 1]);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","isVisible","form","date","delivery","gender","advice","medicines","quantity","price","totalPrice","medicinesDB","stock","computed","reduce","total","medicine","parseFloat","toFixed","methods","openModal","document","body","style","overflow","closeModal","submitForm","console","log","addMedicine","push","removeMedicine","index","splice","calculatePrice","selectedMedicine","find","med","querySearch","queryString","cb","map","item","value","results","filter","createFilter","toLowerCase","includes","handleSelect","length"],"sources":["E:\\2024年春\\SoftwareEngineering\\se_group6_project\\zqs_project_1\\src\\components\\PrescriptionDetails.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal-background\" v-if=\"isVisible\">\r\n    <div class=\"modal-container\">\r\n      <el-form :model=\"form\" class=\"form\" label-width=\"auto\" style=\"max-width: 600px\">\r\n        <el-form-header class=\"form-header\">处方开具</el-form-header>\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"form.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"就诊日期\">\r\n          <el-date-picker v-model=\"form.date\" type=\"date\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio-group v-model=\"form.gender\">\r\n            <el-radio value=\"男\">男</el-radio>\r\n            <el-radio value=\"女\">女</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n\r\n        <el-form-item v-for=\"(medicine, index) in  form.medicines \" :key=\"index\">\r\n          <el-autocomplete v-model=\"medicine.name\" :trigger-on-focus=\"false\" :fetch-suggestions=\"querySearch\"\r\n            placeholder=\"药物名称\" @select=\"handleSelect\" :show-all=\"true\"></el-autocomplete>\r\n          <el-input-number v-model=\"medicine.quantity\" :min=\"0\" :step=\"1\" placeholder=\"数量\"\r\n            @change=\"calculatePrice(medicine)\"></el-input-number>\r\n          <el-row>\r\n            <!-- <el-col :span=\"12\">\r\n              <el-input v-model=\"medicine.price\" disabled placeholder=\"单价\" style=\"margin-left: 10px;\"></el-input>\r\n            </el-col> -->\r\n            <el-col :span=\"12\">\r\n              <el-input v-model=\"medicine.totalPrice\" disabled placeholder=\"总价\" style=\"margin-left: 10px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"removeMedicine(index)\"\r\n            v-if=\"form.medicines.length > 1\">删除药物</el-button>\r\n        </el-form-item>\r\n\r\n\r\n        <el-form-item>\r\n          <el-button @click=\"addMedicine\" type=\"primary\">添加药物</el-button>\r\n        </el-form-item>\r\n        <el-form-item label=\"医师建议\">\r\n          <el-input type=\"textarea\" v-model=\"form.advice\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-input v-model=\"totalPrice\" disabled placeholder=\"总价\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div class=\"button-group\">\r\n        <el-button @click=\"submitForm\" type=\"primary\">提交</el-button>\r\n        <el-button @click=\"closeModal\">退出</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: 'PrescriptionDetails',\r\n  data () {\r\n    return {\r\n      isVisible: false,\r\n      form: {\r\n        name: '',\r\n        date: '',\r\n        delivery: false,\r\n        gender: [],\r\n        advice: '',\r\n        medicines: [{ name: '', quantity: 1, price: 0, totalPrice: 0 }]\r\n      },\r\n      medicinesDB: [\r\n\r\n        { name: '阿司匹林', stock: 100, price: 2.5 },\r\n        { name: '头孢', stock: 50, price: 10.0 },\r\n        { name: '布洛芬', stock: 80, price: 5.0 },\r\n        // 可以添加更多药物数据\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    totalPrice () {\r\n      return this.form.medicines.reduce((total, medicine) => total + parseFloat(medicine.totalPrice || 0), 0).toFixed(2);\r\n    }\r\n  },\r\n  methods: {\r\n    openModal () {\r\n      this.isVisible = true;\r\n      document.body.style.overflow = 'hidden'; // 禁止滚动\r\n    },\r\n    closeModal () {\r\n      this.isVisible = false;\r\n      document.body.style.overflow = ''; // 恢复滚动\r\n    },\r\n    submitForm () {\r\n      // 提交表单逻辑\r\n      console.log('提交表单');\r\n      // 提交后关闭弹窗\r\n      this.closeModal();\r\n    },\r\n    addMedicine () {\r\n      this.form.medicines.push({ name: '', quantity: 1, price: 0, totalPrice: 0 });\r\n    },\r\n    removeMedicine (index) {\r\n      this.form.medicines.splice(index, 1);\r\n    },\r\n    calculatePrice (medicine) {\r\n      const selectedMedicine = this.medicinesDB.find(med => med.name === medicine.name);\r\n      if (selectedMedicine) {\r\n        medicine.price = selectedMedicine.price;\r\n        medicine.totalPrice = (selectedMedicine.price * medicine.quantity).toFixed(2);\r\n      }\r\n    },\r\n\r\n    querySearch (queryString, cb) {\r\n      var medicines = this.medicinesDB.map(item => ({ value: item.name }));  // 现在每个条目都是一个对象，包含 value 属性\r\n      console.log(medicines);\r\n      var results = queryString ? medicines.filter(this.createFilter(queryString)) : medicines;\r\n      console.log(results);\r\n      // 调用回调函数返回建议的结果\r\n      cb(results);\r\n    },\r\n\r\n    createFilter (queryString) {\r\n      return (medicine) => {\r\n        return medicine.value.toLowerCase().includes(queryString.toLowerCase());\r\n      };\r\n    },\r\n\r\n    handleSelect (item) {\r\n      var selectedMedicine = this.medicinesDB.find(med => med.name === item);\r\n      if (selectedMedicine) {\r\n        this.form.medicines[this.form.medicines.length - 1].price = selectedMedicine.price;\r\n        this.calculatePrice(this.form.medicines[this.form.medicines.length - 1]);\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.modal-background {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 5;\r\n}\r\n\r\n.modal-container {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.button-group {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.form-header {\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"mappings":";AAwDA,eAAe;EACbA,IAAI,EAAE,qBAAqB;EAC3BC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,IAAI,EAAE;QACJH,IAAI,EAAE,EAAE;QACRI,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,KAAK;QACfC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE,EAAE;QACVC,SAAS,EAAE,CAAC;UAAER,IAAI,EAAE,EAAE;UAAES,QAAQ,EAAE,CAAC;UAAEC,KAAK,EAAE,CAAC;UAAEC,UAAU,EAAE;QAAE,CAAC;MAChE,CAAC;MACDC,WAAW,EAAE,CAEX;QAAEZ,IAAI,EAAE,MAAM;QAAEa,KAAK,EAAE,GAAG;QAAEH,KAAK,EAAE;MAAI,CAAC,EACxC;QAAEV,IAAI,EAAE,IAAI;QAAEa,KAAK,EAAE,EAAE;QAAEH,KAAK,EAAE;MAAK,CAAC,EACtC;QAAEV,IAAI,EAAE,KAAK;QAAEa,KAAK,EAAE,EAAE;QAAEH,KAAK,EAAE;MAAI;MACrC;MAAA;IAEJ,CAAC;EACH,CAAC;EACDI,QAAQ,EAAE;IACRH,UAASA,CAAA,EAAK;MACZ,OAAO,IAAI,CAACR,IAAI,CAACK,SAAS,CAACO,MAAM,CAAC,CAACC,KAAK,EAAEC,QAAQ,KAAKD,KAAI,GAAIE,UAAU,CAACD,QAAQ,CAACN,UAAS,IAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAACQ,OAAO,CAAC,CAAC,CAAC;IACpH;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAAQA,CAAA,EAAK;MACX,IAAI,CAACnB,SAAQ,GAAI,IAAI;MACrBoB,QAAQ