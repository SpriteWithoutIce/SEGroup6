 <button type=\"button\" @click=\"removeMedicine(index)\" v-if=\"index !== medicines.length - 1\">-</button>\r\n          </div>\r\n          <button type=\"button\" @click=\"addMedicine\">Add Medicine</button>\r\n        </div>\r\n        <button type=\"submit\">Submit</button>\r\n        <button @click=\"closeModal\">Close</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst medicineDatabase = [\r\n  { name: '伸腿瞪眼丸', quantity: '10 颗' },\r\n  { name: '唐森肉', quantity: '20 颗' },\r\n  { name: '馋嘴鱼乌冬面', quantity: '30 课' },\r\n  { name: '麦乐鸡', quantity: '15 颗' },\r\n  { name: '麦旋风', quantity: '10 颗' },\r\n  // 添加更多药物数据...\r\n];\r\n\r\nexport default {\r\n  name: 'PrescriptionDetails',\r\n  data () {\r\n    return {\r\n      isVisible: false,\r\n      patientName: '',\r\n      consultationDate: '',\r\n      patientGender: 'male',\r\n      patientAge: '',\r\n      medicines: [{ name: '', quantity: '' }],\r\n      medicineDatabase: medicineDatabase\r\n    };\r\n  },\r\n  methods: {\r\n    openModal () {\r\n      this.isVisible = true;\r\n    },\r\n    closeModal () {\r\n      this.isVisible = false;\r\n    },\r\n    addMedicine () {\r\n      this.medicines.push({ name: '', quantity: '' });\r\n    },\r\n    removeMedicine (index) {\r\n      this.medicines.splice(index, 1);\r\n    },\r\n    searchMedicine (index) {\r\n      const searchValue = this.medicines[index].name.toLowerCase();\r\n      const foundMedicine = this.medicineDatabase.find(medicine => medicine.name.toLowerCase() === searchValue);\r\n      if (foundMedicine) {\r\n        this.medicines[index].quantity = foundMedicine.quantity;\r\n      } else {\r\n        this.medicines[index].quantity = '';\r\n      }\r\n    },\r\n    submitForm () {\r\n      // Here you can handle form submission, such as sending data to server\r\n      console.log({\r\n        patientName: this.patientName,\r\n        consultationDate: this.consultationDate,\r\n        patientGender: this.patientGender,\r\n        patientAge: this.patientAge,\r\n        medicines: this.medicines\r\n      });\r\n      //提交后设置不可见\r\n      this.isVisible = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n\r\ninput[list] {\r\n  width: calc(100% - 100px);\r\n}\r\n\r\n.medicine-row {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.medicine-row button {\r\n  margin-left: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AA4CA,MAAMA,gBAAe,GAAI,CACvB;EAAEC,IAAI,EAAE,OAAO;EAAEC,QAAQ,EAAE;AAAO,CAAC,EACnC;EAAED,IAAI,EAAE,KAAK;EAAEC,QAAQ,EAAE;AAAO,CAAC,EACjC;EAAED,IAAI,EAAE,QAAQ;EAAEC,QAAQ,EAAE;AAAO,CAAC,EACpC;EAAED,IAAI,EAAE,KAAK;EAAEC,QAAQ,EAAE;AAAO,CAAC,EACjC;EAAED,IAAI,EAAE,KAAK;EAAEC,QAAQ,EAAE;AAAO;AAChC;AAAA,CACD;AAED,eAAe;EACbD,IAAI,EAAE,qBAAqB;EAC3BE,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE,MAAM;MACrBC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,CAAC;QAAER,IAAI,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC,CAAC;MACvCF,gBAAgB,EAAEA;IACpB,CAAC;EACH,CAAC;EACDU,OAAO,EAAE;IACPC,SAAQA,CAAA,EAAK;MACX,IAAI,CAACP,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDQ,UAASA,CAAA,EAAK;MACZ,IAAI,CAACR,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDS,WAAUA,CAAA,EAAK;MACb,IAAI,CAACJ,SAAS,CAACK,IAAI,CAAC;QAAEb,IAAI,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC,CAAC;IACjD,CAAC;IACDa,cAAaA,CAAGC,KAAK,EAAE;MACrB,IAAI,CAACP,SAAS,CAACQ,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IACjC,CAAC;IACDE,cAAaA,CAAGF,KAAK,EAAE;MACrB,MAAMG,WAAU,GAAI,IAAI,CAACV,SAAS,CAACO,KAAK,CAAC,CAACf,IAAI,CAACmB,WAAW,CAAC,CAAC;MAC5D,MAAMC,aAAY,GAAI,IAAI,CAACrB,gBAAgB,CAACsB,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACtB,IAAI,CAA