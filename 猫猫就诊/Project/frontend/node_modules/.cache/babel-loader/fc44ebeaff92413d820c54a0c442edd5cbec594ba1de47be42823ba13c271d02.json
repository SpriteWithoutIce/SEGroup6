{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nconst medicineDatabase = [{\n  name: '伸腿瞪眼丸',\n  quantity: '10 颗'\n}, {\n  name: '唐森肉',\n  quantity: '20 颗'\n}, {\n  name: '馋嘴鱼乌冬面',\n  quantity: '30 课'\n}, {\n  name: '麦乐鸡',\n  quantity: '15 颗'\n}, {\n  name: '麦旋风',\n  quantity: '10 颗'\n}\n// 添加更多药物数据...\n];\nexport default {\n  name: 'PrescriptionDetails',\n  data() {\n    return {\n      isVisible: false,\n      patientName: '',\n      consultationDate: '',\n      patientGender: 'male',\n      patientAge: '',\n      medicines: [{\n        name: '',\n        quantity: ''\n      }],\n      medicineDatabase: medicineDatabase\n    };\n  },\n  methods: {\n    openModal() {\n      this.isVisible = true;\n    },\n    closeModal() {\n      this.isVisible = false;\n    },\n    addMedicine() {\n      this.medicines.push({\n        name: '',\n        quantity: ''\n      });\n    },\n    removeMedicine(index) {\n      this.medicines.splice(index, 1);\n    },\n    searchMedicine(index) {\n      const searchValue = this.medicines[index].name.toLowerCase();\n      const foundMedicine = this.medicineDatabase.find(medicine => medicine.name.toLowerCase() === searchValue);\n      if (foundMedicine) {\n        this.medicines[index].quantity = foundMedicine.quantity;\n      } else {\n        this.medicines[index].quantity = '';\n      }\n    },\n    submitForm() {\n      // Here you can handle form submission, such as sending data to server\n      console.log({\n        patientName: this.patientName,\n        consultationDate: this.consultationDate,\n        patientGender: this.patientGender,\n        patientAge: this.patientAge,\n        medicines: this.medicines\n      });\n      //提交后设置不可见\n      this.isVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["medicineDatabase","name","quantity","data","isVisible","patientName","consultationDate","patientGender","patientAge","medicines","methods","openModal","closeModal","addMedicine","push","removeMedicine","i