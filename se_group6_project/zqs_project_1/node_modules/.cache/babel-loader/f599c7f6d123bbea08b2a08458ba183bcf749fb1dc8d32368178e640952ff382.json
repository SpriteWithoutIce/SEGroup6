{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nexport default {\n  name: 'PrescriptionDetails',\n  setup() {\n    const isVisible = ref(false);\n    const form = ref({\n      name: '',\n      date: '',\n      delivery: false,\n      gender: [],\n      advice: '',\n      medicines: [{\n        name: '',\n        quantity: 1,\n        price: 0,\n        totalPrice: 0\n      }]\n    });\n    const medicinesDB = [{\n      name: '阿司匹林',\n      stock: 100,\n      price: 2.5\n    }, {\n      name: '头孢',\n      stock: 50,\n      price: 10.0\n    }, {\n      name: '布洛芬',\n      stock: 80,\n      price: 5.0\n    }\n    // 可以添加更多药物数据\n    ];\n    const totalPrice = ref(0);\n    function openModal() {\n      isVisible.value = true;\n      document.body.style.overflow = 'hidden'; // 禁止滚动\n    }\n    function closeModal() {\n      isVisible.value = false;\n      document.body.style.overflow = ''; // 恢复滚动\n    }\n    function submitForm() {\n      // 提交表单逻辑\n      console.log('提交表单');\n      // 提交后关闭弹窗\n      closeModal();\n    }\n    function addMedicine() {\n      form.value.medicines.push({\n        name: '',\n        quantity: 1,\n        price: 0,\n        totalPrice: 0\n      });\n    }\n    function removeMedicine(index) {\n      form.value.medicines.splice(index, 1);\n    }\n    function calculatePrice(medicine) {\n      const selectedMedicine = medicinesDB.find(med => med.name === medicine.name);\n      if (selectedMedicine) {\n        medicine.price = selectedMedicine.price;\n        medicine.totalPrice = (selectedMedicine.price * medicine.quantity).toFixed(2);\n      }\n    }\n    function querySearch(queryString, cb) {\n      const results = queryString ? medicinesDB.filter(createFilter(queryString)) : medicinesDB;\n      cb(results);\n    }\n    function createFilter(queryString) {\n      return medicine => {\n        return medicine.name.toLowerCase().indexOf(queryString.toLowerCase()) !== -1;\n      };\n    }\n    return {\n      isVisible,\n      form,\n      medicinesDB,\n      totalPrice,\n      openModal,\n      closeModal,\n      submitForm,\n      addMedicine,\n      removeMedicine,\n      calculatePrice,\n      querySearch\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","isVisible","form","date","delivery","gender","advice","medicines","quantity","price","totalPrice","medicinesDB","stock","openModal","value","document","body","style","overflow","closeModal","submitForm","console","log","addMedicine","push","removeMedicine","index","splice","calculatePrice","medicine","selectedMedicine","find","med","toFixed","querySearch","queryString","cb","results","filter","createFilter","toLowerCase","indexOf"],"sources":["E:\\2024年春\\SoftwareEngineering\\se_group6_project\\zqs_project_1\\src\\components\\PrescriptionDetails.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal-background\" v-if=\"isVisible\">\r\n    <div class=\"modal-container\">\r\n      <el-form :model=\"form\" class=\"form\" label-width=\"auto\" style=\"max-width: 600px\">\r\n        <el-form-header class=\"form-header\">处方开具</el-form-header>\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"form.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"就诊日期\">\r\n          <el-date-picker v-model=\"form.date\" type=\"date\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"Delivery\">\r\n          <el-switch v-model=\"form.delivery\"></el-switch>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio-group v-model=\"form.gender\">\r\n            <el-radio :label=\"'男'\">男</el-radio>\r\n            <el-radio :label=\"'女'\">女</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item v-for=\"(medicine, index) in form.medicines\" :key=\"index\">\r\n          <el-autocomplete v-model=\"medicine.name\" :fetch-suggestions=\"querySearch\" placeholder=\"药物名称\">\r\n            <template #default=\"{ item }\">\r\n              <div class=\"name\">{{ item.name }}</div>\r\n            </template>\r\n          </el-autocomplete>\r\n          <el-input-number v-model=\"medicine.quantity\" :min=\"1\" :step=\"1\" placeholder=\"数量\"\r\n            @change=\"calculatePrice(medicine)\"></el-input-number>\r\n          <el-input v-model=\"medicine.price\" disabled placeholder=\"单价\" style=\"margin-left: 10px;\"></el-input>\r\n          <el-input v-model=\"medicine.totalPrice\" disabled placeholder=\"总价\" style=\"margin-left: 10px;\"></el-input>\r\n          <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"removeMedicine(index)\"\r\n            v-if=\"form.medicines.length > 1\"></el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button @click=\"addMedicine\" type=\"primary\">添加药物</el-button>\r\n        </el-form-item>\r\n        <el-form-item label=\"医师建议\">\r\n          <el-input type=\"textarea\" v-model=\"form.advice\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-input v-model=\"totalPrice\" disabled placeholder=\"总价\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div class=\"button-group\">\r\n        <el-button @click=\"submitForm\" type=\"primary\">提交</el-button>\r\n        <el-button @click=\"closeModal\">退出</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\n\r\nexport default {\r\n  name: 'PrescriptionDetails',\r\n  setup () {\r\n    const isVisible = ref(false);\r\n    const form = ref({\r\n      name: '',\r\n      date: '',\r\n      delivery: false,\r\n      gender: [],\r\n      advice: '',\r\n      medicines: [{ name: '', quantity: 1, price: 0, totalPrice: 0 }]\r\n    });\r\n    const medicinesDB = [\r\n      { name: '阿司匹林', stock: 100, price: 2.5 },\r\n      { name: '头孢', stock: 50, price: 10.0 },\r\n      { name: '布洛芬', stock: 80, price: 5.0 },\r\n      // 可以添加更多药物数据\r\n    ];\r\n\r\n    const totalPrice = ref(0);\r\n\r\n    function openModal () {\r\n      isVisible.value = true;\r\n      document.body.style.overflow = 'hidden'; // 禁止滚动\r\n    }\r\n\r\n    function closeModal () {\r\n      isVisible.value = false;\r\n      document.body.style.overflow = ''; // 恢复滚动\r\n    }\r\n\r\n    function submitForm () {\r\n      // 提交表单逻辑\r\n      console.log('提交表单');\r\n      // 提交后关闭弹窗\r\n      closeModal();\r\n    }\r\n\r\n    function addMedicine () {\r\n      form.value.medicines.push({ name: '', quantity: 1, price: 0, totalPrice: 0 });\r\n    }\r\n\r\n    function removeMedicine (index) {\r\n      form.value.medicines.splice(index, 1);\r\n    }\r\n\r\n    function calculatePrice (medicine) {\r\n      const selectedMedicine = medicinesDB.find(med => med.name === medicine.name);\r\n      if (selectedMedicine) {\r\n        medicine.price = selectedMedicine.price;\r\n        medicine.totalPrice = (selectedMedicine.price * medicine.quantity).toFixed(2);\r\n      }\r\n    }\r\n\r\n    function querySearch (queryString, cb) {\r\n      const results = queryString ? medicinesDB.filter(createFilter(queryString)) : medicinesDB;\r\n      cb(results);\r\n    }\r\n\r\n    function createFilter (queryString) {\r\n      return (medicine) => {\r\n        return (medicine.name.toLowerCase().indexOf(queryString.toLowerCase()) !== -1);\r\n      };\r\n    }\r\n\r\n    return {\r\n      isVisible,\r\n      form,\r\n      medicinesDB,\r\n      totalPrice,\r\n      openModal,\r\n      closeModal,\r\n      submitForm,\r\n      addMedicine,\r\n      removeMedicine,\r\n      calculatePrice,\r\n      querySearch\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal-background {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-container {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.button-group {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.form-header {\r\n  text-align: center;\r\n}\r\n\r\n.name {\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":";AAoDA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,KAAIA,CAAA,EAAK;IACP,MAAMC,SAAQ,GAAIH,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMI,IAAG,GAAIJ,GAAG,CAAC;MACfC,IAAI,EAAE,EAAE;MACRI,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,CAAC;QAAER,IAAI,EAAE,EAAE;QAAES,QAAQ,EAAE,CAAC;QAAEC,KAAK,EAAE,CAAC;QAAEC,UAAU,EAAE;MAAE,CAAC;IAChE,CAAC,CAAC;IACF,MAAMC,WAAU,GAAI,CAClB;MAAEZ,IAAI,EAAE,MAAM;MAAEa,KAAK,EAAE,GAAG;MAAEH,KAAK,EAAE;IAAI,CAAC,EACxC;MAAEV,IAAI,EAAE,IAAI;MAAEa,KAAK,EAAE,EAAE;MAAEH,KAAK,EAAE;IAAK,CAAC,EACtC;MAAEV,IAAI,EAAE,KAAK;MAAEa,KAAK,EAAE,EAAE;MAAEH,KAAK,EAAE;IAAI;IACrC;IAAA,CACD;IAED,MAAMC,UAAS,GAAIZ,GAAG,CAAC,CAAC,CAAC;IAEzB,SAASe,SAAQA,CAAA,EAAK;MACpBZ,SAAS,CAACa,KAAI,GAAI,IAAI;MACtBC,QAAQ,CAACC,IAAI,CAACC,KAAK,CAACC,QAAO,GAAI,QAAQ,EAAE;IAC3C;IAEA,SAASC,UAASA,CAAA,EAAK;MACrBlB,SAAS,CAACa,KAAI,GAAI,KAAK;MACvBC,QAAQ,CAACC,IAAI,CAACC,KAAK,CAACC,QAAO,GAAI,EAAE,EAAE;IACrC;IAEA,SAASE,UAASA,CAAA,EAAK;MACrB;MACAC,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;MACnB;MACAH,UAAU,CAAC,CAAC;IACd;IAEA,SAASI,WAAUA,CAAA,EAAK;MACtBrB,IAAI,CAACY,KAAK,CAACP,SAAS,CAACiB,IAAI,CAAC;QAAEzB,IAAI,EAAE,EAAE;QAAES,QAAQ,EAAE,CAAC;QAAEC,KAAK,EAAE,CAAC;QAAEC,UAAU,EAAE;MAAE,CAAC,CAAC;IAC/E;IAEA,SAASe,cAAaA,CAAGC,KAAK,EAAE;MAC9BxB,IAAI,CAACY,KAAK,CAACP,SAAS,CAACoB,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IACvC;IAEA,SAASE,cAAaA,CAAGC,QAAQ,EAAE;MACjC,MAAMC,gBAAe,GAAInB,WAAW,CAACoB,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACjC,IAAG,KAAM8B,QAAQ,CAAC9B,IAAI,CAAC;MAC5E,IAAI+B,gBAAgB,EAAE;QACpBD,QAAQ,CAACpB,KAAI,GAAIqB,gBAAgB,CAACrB,KAAK;QACvCoB,QAAQ,CAACnB,UAAS,GAAI,CAACoB,gBAAgB,CAACrB,KAAI,GAAIoB,QAAQ,CAACrB,QAAQ,EAAEyB,OAAO,CAAC,CAAC,CAAC;MAC/E;IACF;IAEA,SAASC,WAAUA,CAAGC,WAAW,EAAEC,EAAE,EAAE;MACrC,MAAMC,OAAM,GAAIF,WAAU,GAAIxB,