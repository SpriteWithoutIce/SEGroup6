ute to limit CSS to this component only -->\n<style scoped>\n/*想消除表格下边的那个白线条*/\n.header {\n  font-size: 30px;\n  line-height: 80px;\n  background-color: #f1f1f1;\n}\n\n/*菜单栏*/\n.aside {\n  background-color: wheat;\n}\n\n/*标题的下划线*/\n.subHeader {\n  background-color: cornflowerblue;\n}\n\n/*问诊列表*/\n\n/*最下边的哪个*/\n.footer {\n  background-color: dimgrey;\n  color: white;\n  font-size: 17px;\n  line-height: 30px;\n\n}\n</style>\n"],"mappings":"AAqDA,OAAOA,mBAAkB,MAAO,2BAA0B;AAC1D,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,IAAI,EAAE,QAAQ;MACdC,IAAI,EAAE,CACJ;QACEC,IAAI,EAAE,IAAI;QACVC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,GAAG;QACRC,IAAI,EAAE;MACR,CAAC,EAAE;QACDH,IAAI,EAAE,IAAI;QACVC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,GAAG;QACRC,IAAI,EAAE;MACR,CAAC,EAAE;QACDH,IAAI,EAAE,IAAI;QACVC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,GAAG;QACRC,IAAI,EAAE;MACR;IAEJ;EACF,CAAC;EACDC,UAAU,EAAE;IACVR;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,uBAAsBA,CAAA,EAAK;MACzB,IAAI,CAACC,KAAK,CAACC,mBAAmB,CAACC,SAAS,CAAC,CAAC;IAC5C,CAAC;IACDC,UAASA,CAAGC,KAAK,EAAE;MACjB,IAAI,CAACb,IAAG,GAAK,OAAMa,KAAM,IAAE;IAC7B,CAAC;IACDC,SAAQA,CAAGD,KAAK,EAAEE,GAAG,EAAE;MACrBC,OAAO,CAACC,GAAG,CAAE,SAAQJ,KAAK,IAAIE,GAAG,CAACb,IAAK,EAAC;IAC1C;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}