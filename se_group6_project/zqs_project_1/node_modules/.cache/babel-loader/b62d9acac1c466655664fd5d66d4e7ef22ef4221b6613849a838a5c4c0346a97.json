  <el-form-item label=\"药物/开具数量\">\r\n        <el-form-array v-model=\"formData.medicines\">\r\n          <template v-slot=\"{ fields, add, remove }\">\r\n            <div v-for=\"(field, index) in fields\" :key=\"index\">\r\n              <el-row>\r\n                <el-col :span=\"10\">\r\n                  <el-form-item prop=\"name\">\r\n                    <el-autocomplete v-model=\"field.name\" :fetch-suggestions=\"querySearchMedicine\" placeholder=\"请输入药物名称\"\r\n                      clearable></el-autocomplete>\r\n                  </el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"10\">\r\n                  <el-form-item prop=\"quantity\">\r\n                    <el-input v-model.number=\"field.quantity\" placeholder=\"请输入数量\" clearable></el-input>\r\n                  </el-form-item>\r\n                </el-col>\r\n                <el-col :span=\"4\" class=\"btn-remove\">\r\n                  <el-button type=\"danger\" icon=\"el-icon-delete\" circle @click=\"remove(index)\"\r\n                    v-if=\"fields.length > 1\"></el-button>\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n            <div class=\"btn-add\">\r\n              <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"add\">新增药物</el-button>\r\n            </div>\r\n          </template>\r\n        </el-form-array>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div class=\"dialog-footer\" slot=\"footer\">\r\n      <el-button @click=\"closeModal\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"submitForm\">提交</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport { ElDialog, ElForm, ElFormItem, ElInput, ElDatePicker, ElRadio, ElRadioGroup, ElButton, ElAutocomplete, ElRow, ElCol, ElFormArray } from 'element-plus';\r\nimport 'element-plus/lib/theme-chalk/index.css';\r\n\r\nexport default {\r\n  components: { ElDialog, ElForm, ElFormItem, ElInput, ElDatePicker, ElRadio, ElRadioGroup, ElButton, ElAutocomplete, ElRow, ElCol, ElFormArray },\r\n  data () {\r\n    return {\r\n      isVisible: false,\r\n      formData: {\r\n        patientName: '',\r\n        consultationDate: '',\r\n        patientGender: 'male',\r\n        patientAge: '',\r\n        medicines: [{ name: '', quantity: '' }]\r\n      },\r\n      rules: {\r\n        patientName: [{ required: true, message: '请输入姓名', trigger: 'blur' }],\r\n        consultationDate: [{ required: true, message: '请选择日期', trigger: 'change' }],\r\n        patientGender: [{ required: true, message: '请选择性别', trigger: 'change' }],\r\n        patientAge: [{ required: true, message: '请输入年龄', trigger: 'blur' }]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    openModal () {\r\n      this.isVisible = true;\r\n    },\r\n    closeModal () {\r\n      this.isVisible = false;\r\n    },\r\n    add () {\r\n      this.formData.medicines.push({ name: '', quantity: '' });\r\n    },\r\n    remove (index) {\r\n      this.formData.medicines.splice(index, 1);\r\n    },\r\n    submitForm () {\r\n      this.$refs.form.validate(valid => {\r\n        if (valid) {\r\n          console.log(this.formData);\r\n          this.isVisible = false;\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    querySearchMedicine (queryString, cb) {\r\n      const results = queryString ? this.medicineDatabase.filter(item => item.name.toLowerCase().includes(queryString.toLowerCase())) : this.medicineDatabase;\r\n      cb(results.map(item => item.name));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dialog-footer {\r\n  padding: 10px 20px;\r\n  text-align: right;\r\n}\r\n\r\n.btn-remove {\r\n  text-align: right;\r\n}\r\n\r\n.btn-add {\r\n  margin-top: 20px;\r\n  text-align: right;\r\n}\r\n</style>\r\n"],"mappings":";AAwDA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAQ,EAAEC,MAAM,EAAEC,UAAU,EAAEC,OAAO,EAAEC,YAAY,EAAEC,OAAO,EAAEC,YAAY,EAAEC,QAAQ,EAAEC,cAAc,EAAEC,KAAK,EAAEC,KAAK