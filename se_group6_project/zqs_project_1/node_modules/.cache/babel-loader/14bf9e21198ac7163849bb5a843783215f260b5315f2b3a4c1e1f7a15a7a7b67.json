ptionDetails\">\r\n    <h1>处方开具</h1>\r\n    <el-form :model=\"prescriptionForm\" ref=\"prescriptionForm\" :rules=\"rules\" label-width=\"120px\">\r\n      <el-form-item label=\"患者姓名\" prop=\"patientName\">\r\n        <el-input v-model=\"prescriptionForm.patientName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"患者年龄\" prop=\"patientAge\">\r\n        <el-input v-model.number=\"prescriptionForm.patientAge\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"药品名称\" prop=\"medicineName\">\r\n        <el-input v-model=\"prescriptionForm.medicineName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"药品剂量\" prop=\"medicineDosage\">\r\n        <el-input v-model.number=\"prescriptionForm.medicineDosage\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"医生姓名\" prop=\"doctorName\">\r\n        <el-input v-model=\"prescriptionForm.doctorName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('prescriptionForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('prescriptionForm')\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      prescriptionForm: {\r\n        patientName: '',\r\n        patientAge: null,\r\n        medicineName: '',\r\n        medicineDosage: null,\r\n        doctorName: ''\r\n      },\r\n      rules: {\r\n        patientName: [{ required: true, message: '请输入患者姓名', trigger: 'blur' }],\r\n        patientAge: [\r\n          { required: true, message: '请输入患者年龄', trigger: 'blur' },\r\n          { type: 'number', message: '年龄必须为数字值' }\r\n        ],\r\n        medicineName: [{ required: true, message: '请输入药品名称', trigger: 'blur' }],\r\n        medicineDosage: [\r\n          { required: true, message: '请输入药品剂量', trigger: 'blur' },\r\n          { type: 'number', message: '药品剂量必须为数字值' }\r\n        ],\r\n  